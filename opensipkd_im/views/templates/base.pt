                <li><a href="/imgw/conf">Konfigurasi</a></li>
              </ul>
            </li>

            <li class="dropdown" tal:attributes="class request.matched_route.name in ['user', 'user-add', 'user-edit', 'user-delete', 'group', 'group-add', 'group-edit', 'group-delete'] and 'active'">
